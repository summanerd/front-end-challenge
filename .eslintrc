{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "import/prefer-default-export": "off",
    "prefer-arrow-callback": "off",
    "react/jsx-props-no-spreading": "off"
  },
  "globals": {
    "document": true
  },
  "overrides": [
    {
      "files": ["*.test.js"],
      "globals": {
        "before": "readonly",
        "after": "readonly",
        "beforeEach": "readonly",
        "afterEach": "readonly",
        "describe": "readonly",
        "it": "readonly"
      },
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ]
}